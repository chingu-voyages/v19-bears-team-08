{
  "root": true,
  "env": {
    "browser": true,
    "node": true
  },
  "extends": [
    "@nuxtjs/eslint-config-typescript",
    "prettier",
    "prettier/vue",
    "plugin:nuxt/recommended",
    "plugin:prettier/recommended"
  ],
  "plugins": ["prettier"],
  "rules": {
    "camelcase": "off",
    "no-console": "warn",
    "no-unused-vars": "off",
    "vue/attribute-hyphenation": ["error", "never"]
  }
}
